@article{reinekingConstrainPerformRegularization2006,
  title = {Constrain to Perform: {{Regularization}} of Habitat Models},
  author = {Reineking, Björn and Schröder, Boris},
  date = {2006-03},
  journaltitle = {Ecological Modelling},
  volume = {193},
