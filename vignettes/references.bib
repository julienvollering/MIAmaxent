@article{randinAreNichebasedSpecies2006,
  title = {Are Niche-Based Species Distribution Models Transferable in Space?},
  author = {Randin, Christophe F. and Dirnböck, Thomas and Dullinger, Stefan and Zimmermann, Niklaus E. and Zappa, Massimiliano and Guisan, Antoine},
  date = {2006},
  journaltitle = {Journal of Biogeography},
  volume = {33},
